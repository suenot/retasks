# Your GitHub personal access token with repo scope
# Create at: https://github.com/settings/tokens
GITHUB_TOKEN=your_token_here

# GitHub repository to sync in format owner/repo
GITHUB_REPO=username/repository

# Directory for storing issues (optional, default: ./issues)
ISSUES_DIR=./issues

# Sync interval in seconds when using watch mode (optional, default: 300)
SYNC_INTERVAL=300 